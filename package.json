{
  "name": "lifehub",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "check:no-storage": "rg -n --no-ignore --hidden --glob '!*node_modules*' --glob '!**/.next/**' --glob '!**/coverage/**' --glob '!**/dist/**' 'localStorage|sessionStorage' || true",
    "verify-migration": "npx tsx scripts/verify-localstorage-migration.ts",
    "migration:check": "npx tsx scripts/verify-localstorage-migration.ts",
    "prebuild": "echo 'Skipping migration check for deployment'",
    "dev": "next dev",
    "dev:server": "node server.js",
    "build": "NODE_OPTIONS='--max-old-space-size=6144' next build",
    "start": "next start",
    "start:server": "NODE_ENV=production node server.js",
    "lint": "next lint --max-warnings 9999",
    "lint:ci": "npm run lint && npm run migration:check",
    "type-check": "NODE_OPTIONS='--max-old-space-size=4096' tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "test:domains": "tsx scripts/test-domains.ts",
    "verify:schema": "tsx scripts/verify-schema-tables.ts",
    "seed:health": "tsx scripts/seed-health-data.ts",
    "seed:insurance": "tsx scripts/seed-insurance-data.ts",
    "verify:all": "bash scripts/run-schema-verification.sh",
    "test:health-crud": "tsx scripts/test-health-domain.ts",
    "test:insurance-crud": "tsx scripts/test-insurance-domain.ts",
    "qa:report": "tsx scripts/generate-qa-report.ts",
    "debug:dashboard": "tsx scripts/debug-dashboard-data.ts",
    "setup:push": "node scripts/setup-push-notifications.js",
    "setup:plaid": "bash scripts/setup-plaid.sh",
    "test:ai-commands": "tsx scripts/test-ai-commands.ts",
    "test:ai-quick": "bash scripts/quick-test.sh",
    "validate": "npm run type-check && npm run lint && npm run test"
  },
